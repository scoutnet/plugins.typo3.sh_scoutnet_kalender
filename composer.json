	"homepage": "https://bitbucket.org/scoutnet/plugins.typo3.sh_scoutnet_kalender",
	"authors": [
		{
			"name": "Stefan Horst",
			"email": "muetze@scoutnet.de",
			"homepage": "https://www.scoutnet.de",
			"role": "Developer"
		}
	],
	"license": ["GPL-2.0+"],
